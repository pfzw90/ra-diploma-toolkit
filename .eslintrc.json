{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base",
        "plugin:react-redux/recommended"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint",
        "react-redux"
    ],
    "rules": {
        "no-console": "off",
        "no-import-cycle": "off",
        "no-param-reassign": ["error", { "props": false }],
        "react-redux/useSelector-prefer-selectors": "off",
        "no-use-before-define": "off"
    }
}
